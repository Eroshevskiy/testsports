<Window x:Class="testSport.Pages.Autor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:testSport.Pages"
        mc:Ignorable="d"
        Title="Autor" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock>Логин</TextBlock>
            <TextBox x:Name="login"></TextBox>
            <TextBlock>Пароль</TextBlock>
            <PasswordBox x:Name="password"></PasswordBox>
            <Button Background="Aqua" Click="autorClick">Войти</Button>
        </StackPanel>
        
    </Grid>
</Window>
